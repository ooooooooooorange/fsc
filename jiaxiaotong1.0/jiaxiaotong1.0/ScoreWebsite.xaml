<Window x:Class="jiaxiaotong1._0.ScoreWebsite"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:jiaxiaotong1._0"
        mc:Ignorable="d"
        Title="ScoreWebsite" Height="774" Width="1500">
    <Window.Background>
        <ImageBrush ImageSource="背景1.png"/>
    </Window.Background>
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="343*"/>
            <ColumnDefinition Width="755*"/>
            <ColumnDefinition Width="395*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="29*"/>
            <RowDefinition Height="157*"/>
        </Grid.RowDefinitions>
        <Label x:Name="label" Content="查看成绩界面" Grid.Column="1" Margin="214.103,10,128.4,10" FontSize="48" FontWeight="Bold"/>
        <DataGrid x:Name="dataGrid" Grid.ColumnSpan="3" Grid.Row="1" FontSize="36">
            <DataGrid.Columns>
                <DataGridTextColumn Header="等级" FontSize="48"/>
                <DataGridTextColumn Header="科目"/>
                <DataGridTextColumn Header="时间"/>
                <DataGridTextColumn Header="成绩"/>
            </DataGrid.Columns>
        </DataGrid>

    </Grid>
</Window>
